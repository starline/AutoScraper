{
  "manifest_version": 3,
  "name": "Auto Info Parser",
  "version": "1.0",
  "description": "Извлекает информацию с iaai.com и copart.com и копирует в буфер.",
  "permissions": ["scripting", "clipboardWrite", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
